{
    "name": "realtimesql",
    "version": "1.0.0",
    "dependencies": {

        "@sentry/node": "^5.20.1",
        "babel-node": "0.0.1-security",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "csv-express": "^1.2.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.1.9",
        "memory-cache": "^0.2.0",
        "moment": "^2.25.3",
        "mysql": "^2.18.1",
        "path": "^0.12.7",
        "readline": "^1.3.0",
        "stream": "0.0.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.9.6",
        "@babel/node": "^7.8.7",
        "@babel/preset-env": "^7.9.6",
        "nodemon": "^2.0.3",
        "prettier": "^2.0.5"
    },
    "scripts": {
        "start": "nodemon --exec babel-node src/index.js",
        "stop": "pm2 stop metapi",
        "start:prod": "pm2 startOrRestart --name metapi --interpreter babel-node dist/index.js",
        "build": "babel src -d dist",
        "sexy-code": "prettier . --write",
        "docker:dev:up": "docker-compose build app && docker-compose up",
        "docker:dev:start": "docker-compose exec app npm run start"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "semi": true
    }
}
